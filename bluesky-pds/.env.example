# Database host: use your Cloud SQL instance IP or private IP
DB_HOST=your-cloudsql-ip-or-private-ip

# Database port: usually 5432 for PostgreSQL
DB_PORT=5432

# Database name: the name of your PostgreSQL database
DB_NAME=yourdbname

# Database user: the username for your database
DB_USER=yourdbuser

# Database password: the password for your database user
DB_PASSWORD=yourdbpassword